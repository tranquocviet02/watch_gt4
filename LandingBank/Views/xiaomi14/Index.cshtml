@section scripts {
    <!-- Swiper JS -->
    <script src="~/Content/xiaomi14/js/swiper-bundle.min.js"></script>
    <script>
        document.addEventListener("DOMContentLoaded", function () {
            const box = document.getElementById("box");
            const box1 = document.getElementById("box1");
            const box2 = document.getElementById("box2");
            const box3 = document.getElementById("box3");

            window.addEventListener("scroll", function () {
                const scrollPosition = window.scrollY;

                // Điều chỉnh opacity dựa trên vị trí cuộn
                box1.style.opacity = 1 - scrollPosition / 50;
                box2.style.opacity = scrollPosition / 50;
                box3.style.opacity = scrollPosition / 50;
                // Kiểm tra khi nào box nằm trong tầm nhìn
                if (scrollPosition > box.offsetTop) {
                    box.classList.add("matrix");
                } else {
                    box.classList.remove("matrix");
                }
                // Kiểm tra khi nào cuộn qua box
                if (scrollPosition > box.offsetTop + box.offsetHeight) {
                    box.classList.remove("matrix");
                }
            });
        });
    </script>

    <script>
        $(document).ready(function () {
            $('#showMoreBtn').click(function () {
                $('#popup-show-program').css('display', 'block');
                $('body').addClass('no-scroll-body');
            });

            $('.popup-btn-close').click(function () {
                $('#popup-show-program').css('display', 'none');
                $('body').removeClass('no-scroll-body');
            });

            $('#showlist').click(function () {
                $('#popup-btn-list').css('display', 'block');
                $('body').addClass('no-scroll-body');
            });

            $('.popup-btn-close-list').click(function () {
                $('#popup-btn-list').css('display', 'none');
                $('body').removeClass('no-scroll-body');
            });
        });

    </script>

    <script>
        var swiper = new Swiper(".mySwiper", {
            zoom: true,
            navigation: {
                nextEl: ".swiper-button-next",
                prevEl: ".swiper-button-prev",
            },
            pagination: {
                el: ".swiper-pagination",
                clickable: true,
            },
            autoplay: {
                delay: 10000, // Thời gian chờ giữa các slide (miligiây)
                disableOnInteraction: false, // Tự động dừng khi người dùng tương tác với slider
            },
        });

    </script>

}