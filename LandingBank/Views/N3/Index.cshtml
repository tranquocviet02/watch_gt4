@{
    ViewBag.Title = "[05 - 15/01] Đặt trước Redmi Note 13 Series - Sự lựa chọn hoàn hảo cho dịp năm mới";
    @* Layout = "~/Views/Shared/_layout_Desktop.cshtml"; *@
}

@section meta {
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1,minimum-scale=1,user-scalable=no">
    <meta name="description"
        content="[05 - 15/01] Đăng ký trước Redmi Note 13 Series - Nhân ưu đãi ngay. Giảm ngay 500.000đ trên giá sản phẩm + Trả góp 0% + Bảo hành 18 tháng">
    <meta property="og:image" content="~/Content/n3/img/thumb-redmi-note-13-new.jpg">
    <meta property="og:url" content="https://hoanghamobile.com/dang-ky-xiaomi-redmi-note-13-series">

    <link rel="stylesheet" href="~/Content/n3/js-css/swiper-bundle.min.css" />
    <script src="~/Content/n3/js/jquery-3.5.1.min.js?@DateTime.Now"></script>
    <link rel="stylesheet" href="~/Content/n3/css/bootstrap.min.css?@DateTime.Now">
    <link rel="stylesheet" href="~/Content/n3/js-css/web_Desktop.css?@DateTime.Now">
    <link rel="stylesheet" href="~/Content/n3/js-css/style.css?@DateTime.Now">
    <style>
        .note p {
    font-size: max(0.8375vw,12px);
    margin-top: 0.3125vw;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    color: black;
    display: box;
    /* display: -webkit-box; */
    font-weight: 400;
    line-height: 1.25;
    margin: 0;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: inherit;
    word-break: break-word;
    padding: 10px;
        }
    .btn-tab{
    border-radius: 1000px;
    box-shadow: 0 1px 4px 0 rgba(10,10,10,.15);
     border: 1px solid var(--Gray-300,#E1E4E6);
    color: var(--gray-500,#6A737A);
    }
    </style>
}


<main class="main-content W-100">

    <section class="Content-v1 max-w-1200px w-100" style="background: white;">
        <div class="w-100 position-relative">
            <img class="w-100" src="~/Content/n3/img/RMN13_HIDDEN DT.jpg?@DateTime.Now">
            <div class="position-absolute start-45 bottom-20px d-flex-center">
                <a class="button-add-orange btn-buy btn-tab mi-btn__text color-white"
                href="https://hoanghamobile.com/dien-thoai/redmi-note-13-6gb-128gb-chinh-hang?buy=1">Đặt Trước Redmi Note 13</a>
                <a class="button-add-orange btn-buy btn-tab mi-btn__text " style=" color: var(--Gray-600,#6A737A);    background: white;"
                href="https://hoanghamobile.com/dien-thoai/redmi-note-13-pro-plus-8gb-256gb-chinh-hang?buy=1">Đặt Trước Redmi Note 13 Pro+ 5G</a>
            </div>
        </div>
        <p class="mi-btn__text resetfont " style="margin-top: 20px;font-size: 19px;text-align: center;color: black;">TIN
            ĐỒN: TÍNH NĂNG VƯỢT TRỘI CỦA REDMI NOTE 13 SERIES</p>
        <div class="d-flex text-align-center button-slide margin-20px-0 ">
            <button class="resetfont div-banner-item btn-add-all click-oppo-n3 text-btn-add Reserve active">
                Redmi Note 13
            </button>
            <button class="resetfont div-banner-item btn-add-all click-oppo-n3-pro text-btn-add ">
                Redmi Note 13 Pro + 5G
            </button>
        </div>

        <section class="w-100 flex-column-center" id="oppo-n3">
            <img class="w-50" src="~/Content/n3/img/Redmi-note-13.jpg?@DateTime.Now">
            <div class="swiper mySwiper w-100">
                <div class="swiper-wrapper">
                    <div class="swiper-slide">
                        <img class="w-100" src="~/Content/n3/img/Note 13/tin đồn redmi note 13-05.jpg?@DateTime.Now">
                    </div>
                    <div class="swiper-slide">
                        <img class="w-100" src="~/Content/n3/img/Note 13/tin đồn redmi note 13-06.jpg?@DateTime.Now">
                    </div>
                    <div class="swiper-slide">
                        <img class="w-100" src="~/Content/n3/img/Note 13/tin đồn redmi note 13-07.jpg?@DateTime.Now">
                    </div>
                </div>
                @* <div class="swiper-button-next"></div>
                <div class="swiper-button-prev"></div> *@
                <button class="start_css swiper-button-prev" type="button" onclick="plusDivs_accessory(-1)" tabindex="0"
                    aria-label="Previous slide" aria-controls="product-container-compare">
                    <svg class="start_css" xmlns="http://www.w3.org/2000/svg" width="50" height="50" viewBox="0 0 50 50"
                        fill="none">
                        <rect width="50" height="50" rx="25" fill="#DBF9EB"></rect>
                        <path d="M31 13L19 25L31 37" stroke="#009981" stroke-width="3"></path>
                    </svg>
                </button>
                <button class="start_css swiper-button-next" type="button" onclick="plusDivs_accessory(1)" tabindex="0"
                    aria-label="Next slide" aria-controls="product-container">
                    <svg class="start_css" xmlns="http://www.w3.org/2000/svg" width="50" height="50" viewBox="0 0 50 50"
                        fill="none">
                        <rect width="50" height="50" rx="25" fill="#DBF9EB"></rect>
                        <path d="M19 37L31 25L19 13" stroke="#009981" stroke-width="3"></path>
                    </svg>
                </button>

                <div class="swiper-pagination d-none"></div>
            </div>
            <div class="flex-column-center padding-20px-0">
                <a class="button-add-orange btn-buy btn-tab mi-btn__text color-white"
                    href="https://hoanghamobile.com/dien-thoai/redmi-note-13-6gb-128gb-chinh-hang?buy=1">Đặt Trước Redmi Note 13</a>
            </div>
        </section>

        <section class="w-100 flex-column-center" id="oppo-n3-pro" style="display: none;">
            <img class="w-50" src="~/Content/n3/img/Redmi-note-13-pro.jpg?@DateTime.Now">
            <div class="swiper mySwiper w-100">
                <div class="swiper-wrapper">
                    <div class="swiper-slide">
                        <img class="w-100"
                            src="~/Content/n3/img/Note 13 Pro/tin đồn redmi note 13-11.jpg?@DateTime.Now">
                    </div>
                    <div class="swiper-slide">
                        <img class="w-100"
                            src="~/Content/n3/img/Note 13 Pro/tin đồn redmi note 13-12.jpg?@DateTime.Now">
                    </div>
                    <div class="swiper-slide">
                        <img class="w-100"
                            src="~/Content/n3/img/Note 13 Pro/tin đồn redmi note 13-13.jpg?@DateTime.Now">
                    </div>
                </div>
                @* <div class="swiper-button-next"></div>
                <div class="swiper-button-prev"></div> *@
                <button class="start_css swiper-button-prev" type="button" onclick="plusDivs_accessory(-1)" tabindex="0"
                    aria-label="Previous slide" aria-controls="product-container-compare">
                    <svg class="start_css" xmlns="http://www.w3.org/2000/svg" width="50" height="50" viewBox="0 0 50 50"
                        fill="none">
                        <rect width="50" height="50" rx="25" fill="#DBF9EB"></rect>
                        <path d="M31 13L19 25L31 37" stroke="#009981" stroke-width="3"></path>
                    </svg>
                </button>
                <button class="start_css swiper-button-next" type="button" onclick="plusDivs_accessory(1)" tabindex="0"
                    aria-label="Next slide" aria-controls="product-container">
                    <svg class="start_css" xmlns="http://www.w3.org/2000/svg" width="50" height="50" viewBox="0 0 50 50"
                        fill="none">
                        <rect width="50" height="50" rx="25" fill="#DBF9EB"></rect>
                        <path d="M19 37L31 25L19 13" stroke="#009981" stroke-width="3"></path>
                    </svg>
                </button>

                <div class="swiper-pagination d-none"></div>
            </div>
            <div class="flex-column-center padding-20px-0">
                <a class="button-add-orange btn-buy btn-tab mi-btn__text color-white"
                    href="https://hoanghamobile.com/dien-thoai/redmi-note-13-pro-plus-8gb-256gb-chinh-hang?buy=1">Đặt Trước Redmi Note 13 Pro+ 5G</a>
            </div>
        </section>


        <div class="w-100 d-flex-center background-black">
            <video autoplay controls muted preload="auto" class="w-35 pc_video lazyloading">
                <source src="~/Content/n3/video/video n13.mp4" type="video/mp4">
                Your browser does not support the video tag.
            </video>
        </div>

        <section class=" max-w-1200px w-100 " >
           <div class="note" style="padding: 20px;">
            <p> <strong>Chương trình Pre-order Note 13 series</strong> <br> 
            - Thời gian Pre-Order: Từ 05-15/01 <br>
            - Số tiền cọc: 500.000đ/ máy <br>
            - Thời gian trả hàng: Từ 16/01 -30/01 <br> <br>
            <strong>( Mở bán sớm 18h 15/01 tại Hoàng Hà Tam Trinh) <br>
            Xiaomi Redmi Note 13 6GB/128GB giá dự kiến 4.890.000 
            </strong> <br> 
            - Quà tặng hãng: Phiếu mua hàng 500.000 <br> <br>
           <strong> Xiaomi Redmi Note 13 Pro+ 5G 8GB/256GB giá dự kiến 10.990.000 </strong><br>
            - Quà tặng hãng: Phiếu mua hàng 500.000 <br> <br>
           <strong>Giá tôt nhất liên hệ 1900.2091</strong> 
            </p>
           </div>
        </section>

    </section>

</main>



@section scripts {
    <!-- Swiper JS -->
    <script src="~/Content/n3/js-css/swiper-bundle.min.js"></script>
    <script>
        if (window.location.href === 'https://hoanghamobile.com/dang-ky-xiaomi-redmi-note-13-series') {
            window.history.replaceState({}, document.title, '/xiaomi-redmi-note-13-series');
        }

    </script>

    <!-- Initialize Swiper -->
    <script>
        var swiper = new Swiper(".mySwiper", {
            loop: true, // Lặp vô hạn các slide
            zoom: true,
            navigation: {
                nextEl: ".swiper-button-next",
                prevEl: ".swiper-button-prev",
            },
            pagination: {
                el: ".swiper-pagination",
                clickable: true,
            },
            autoplay: {
                delay: 5000, // Thời gian chờ giữa các slide (miligiây)
                disableOnInteraction: false, // Tự động dừng khi người dùng tương tác với slider
            },
        });


    </script>

    

    <script src="~/Content/n3/js-css/web-n3.js?@DateTime.Now"></script>
}