<script>
$(document).ready(function () {
    function toggleVisibility(targetClass, targetDivId) {
        var targetDiv = $(targetDivId);

        // Check if the targetDiv is currently set to display: flex
        var isFlexDisplay = targetDiv.css('display') === 'flex';

        // If it's not already displayed as flex, then toggle the visibility
        if (!isFlexDisplay) {
            // Hide all elements with the targetClass
            $(targetClass).hide();

            // Show and set display to 'flex' for the targetDiv
            targetDiv.show().css('display', 'flex');
        }
    }

    $('.toggle').click(function () {
        toggleVisibility('.hidden-div', $(this).data('target'));
    });
});


</script>
<script>
    $(document).ready(function () {
        function toggleVisibility(targetClass, targetDivId) {
            var targetDiv = $(targetDivId);

            // Kiểm tra xem phần tử đang được hiển thị hay không
            if (targetDiv.is(':visible')) {
                // Nếu đang hiển thị, bạn có thể thực hiện một hành động khác ở đây
                console.log('Đang hiển thị, không ẩn');
            } else {
                // Nếu không hiển thị, ẩn tất cả và hiển thị div tương ứng
                $(targetClass).not(targetDivId).hide();
                targetDiv.show();
            }
        }


        $('.s24').click(function () {
