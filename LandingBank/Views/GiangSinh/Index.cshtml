    <style>
        .d-none-important {
            display: none !important;
        }

        .d-block-important {
            display: block !important;
        }

        .sanpham_sale {
            position: absolute;
            top: 200px;
            left: 5px;
            background: #ff6801;
            border-radius: 40px;
            width: 75%;
            /* padding: 0; */
            padding: 5px 5px;
            margin: 0;
            color: #fff;
            font-weight: bold;
            font-size: 13px;
            /* height: 17px; */
            text-align: center;
            z-index: 2;
            display: none;
        }

        .note p {
            font-size: max(0.8375vw, 12px);
            margin-top: 0.3125vw;
            -webkit-box-orient: vertical;
            -webkit-line-clamp: 2;
            color: white;
            display: box;
            /* display: -webkit-box; */
            font-weight: 400;
            line-height: 1.25;
            margin: 0;
            overflow: hidden;
            text-overflow: ellipsis;
            white-space: inherit;
            word-break: break-word;
        }
    </style>
    <script type="text/javascript">
        $(document).ready(function () {
            $.post('/Ajax/Price', { "id": "396,1689,3210,2572,3408,3206,3897,531,902,3939,3537,3266,3180,3228,3087,2831,3015,2110,3838,1659,1658,1183,1678,3211,3402,3932,3725,1231,3333,3652,3685,1703,3510,3395,3216,2892,3163,3627,3945,3553,3203,3411,3923,3844,3418,3410,3171,3856,3678,3869" }, function (data) {
                $(data).each(function (index, value) {
                    var priceContainer = $('#idsanpham_' + value.ID);
                    if (priceContainer.length > 0) {
                        var formattedPrice = parseFloat(value.BestPrice).toLocaleString('vi-VN', { style: 'currency', currency: 'VND' });
                        priceContainer.html('<li class="title-price formatted-number">' + formattedPrice + '</li>');
                    }

                    var priceLastPrice = $('#idsanpham_LastPrice_' + value.ID);
                    if (priceLastPrice.length > 0) {
                        var lastPrice = parseFloat(value.LastPrice);
                        if (isNaN(lastPrice)) {
                            lastPrice = '';
                        }
                        var formattedPrice = lastPrice.toLocaleString('vi-VN', { style: 'currency', currency: 'VND' });
                        priceLastPrice.html('<li class="title-price formatted-number" style="color: var(--black-black-4, #C9C9C9);font-size: max(1vw,11px);">' + formattedPrice + '</li>');
                    }

                    var priceSale = $('#idsanpham_sale_' + value.ID);
                    if (priceSale.length > 0) {
                        var salePrice = parseFloat(value.LastPrice) - parseFloat(value.BestPrice);
                        if (isNaN(salePrice) || salePrice === 0) {
                            priceSale.addClass("d-none-important");
                        } else {
                            priceSale.removeClass("d-none-important").addClass("d-block-important");
                            var formattedSalePrice = salePrice.toLocaleString('vi-VN', { style: 'currency', currency: 'VND' });
                            priceSale.html('<li class="icon-flash2"> Giảm ' + formattedSalePrice + '</li>');
                        }
                    }

					//add price note
					if (priceSale.length > 0) {

						var btnContainer =  $(priceSale).parent();
						var actionContainer = $(btnContainer).find(".site-goods-sku__info");
						var lastActionHtml = $(actionContainer).html();

						if(value.PriceNote) {
							lastActionHtml += '<p style="    font-style: italic;color: #f50;padding: 8px 0;">'+value.PriceNote+'</p>';
							$(actionContainer).html(lastActionHtml);
						}
					}
                });
            });
        });


    </script>
}

