@section scripts {
    <script src="~/Content/MerryChristmas/js-css/Scroll.js?@DateTime.Now"></script>
    <script>
// Lấy thời gian hiện tại
const currentTime = new Date().getTime();

// Thời gian bắt đầu và kết thúc (đổi thành timestamp)
const startTime = new Date('2023-12-18').getTime(); // Timestamp của ngày bắt đầu
const endTime = new Date('2023-12-25').getTime(); // Timestamp của ngày kết thúc

// Lấy danh sách các phần tử có class là "scheduledDiv"
const scheduledDivs = document.getElementsByClassName('scheduledDiv');

// Duyệt qua từng phần tử trong danh sách và xử lý hiển thị dựa trên thời gian
for (let i = 0; i < scheduledDivs.length; i++) {
    const div = scheduledDivs[i];
    if (currentTime >= startTime && currentTime <= endTime) {
        // Nếu thỏa mãn điều kiện, hiển thị div
        div.style.display = 'flex';
    } else {
        // Nếu không thỏa mãn, ẩn div
        div.style.display = 'none';
    }
}

const scheduledDivs_block = document.getElementsByClassName('scheduledDiv_block');

// Duyệt qua từng phần tử trong danh sách và xử lý hiển thị dựa trên thời gian
for (let i = 0; i < scheduledDivs_block.length; i++) {
    const div = scheduledDivs_block[i];
    if (currentTime >= startTime && currentTime <= endTime) {
        // Nếu thỏa mãn điều kiện, hiển thị div
        div.style.display = 'none';
    } else {
        // Nếu không thỏa mãn, ẩn div
        div.style.display = 'flex';
    }
}
    </script>
}
