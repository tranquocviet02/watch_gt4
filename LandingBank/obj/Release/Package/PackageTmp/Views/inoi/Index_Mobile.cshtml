@{
    ViewBag.Title = "Index_Mobile";
    Layout = "~/Views/Shared/_Layout_Mobile.cshtml";
}

@section meta {
    <script src="https://hoanghamobile.com/Content/web/js/jquery-3.5.1.min.js"></script>
    <link rel="stylesheet" href="~/Content/inoi/js-css/web_Mobile.css?@DateTime.Now">
    <link rel="stylesheet" href="~/Content/inoi/js-css/inoi.css?@DateTime.Now">
    <link rel="stylesheet" href="~/Content/inoi/css/bootstrap.min.css?@DateTime.Now">

    
<script type="text/javascript">
    $(document).ready(function () {
        $.post('/Ajax/Price', { "id": "4084,4085,4086,4087" }, function (data) {
            $(data).each(function (index, value) {
                var priceContainer = $('#idsanpham_' + value.ID);
                if (priceContainer.length > 0) {
                    var formattedPrice = parseFloat(value.BestPrice).toLocaleString('vi-VN', { style: 'currency', currency: 'VND' });
                    if (!isNaN(value.BestPrice) && value.BestPrice !== null && value.BestPrice !== undefined && value.BestPrice !== "") {
                        priceContainer.html('<li class="title-price formatted-number resetfont">' + formattedPrice + '</li>');
                    } else {
                        priceContainer.html('');
                    }
                }

                var priceLastPrice = $('#idsanpham_LastPrice_' + value.ID);
                if (priceLastPrice.length > 0) {
                    var lastPrice = parseFloat(value.LastPrice);
                    var formattedLastPrice = isNaN(lastPrice) || !lastPrice ? 0 : lastPrice.toLocaleString('vi-VN', { style: 'currency', currency: 'VND' });
                    if (!isNaN(value.LastPrice) && value.LastPrice !== null && value.LastPrice !== undefined && value.LastPrice !== "") {
                        priceLastPrice.html('<li class="title-price formatted-number">' + formattedLastPrice + '</li>');
                    } else {
                        priceLastPrice.html('');
                    }
                }


                var Image_ck = $('#idsanpham_Image_' + value.ID);
                if (Image_ck.length > 0) {
                    var arrayImages = JSON.parse(value.Image);
                    var imageUrl = '/i/productlist/ts' + arrayImages[0].Url
                    Image_ck.attr('src', imageUrl);
                }

                var title_ck = $('#idsanpham_title_' + value.ID);
                if (title_ck.length > 0) {
                    var title = value.Title;
                    var TitleString = title.replace('- Chính hãng', '');
                    title_ck.html(TitleString);
                }

                var href_ck = $('#idsanpham_href_' + value.ID);
                if (href_ck.length > 0) {
                    var hrefUrl = value.UID + '?buy=1'; // Corrected variable name to hrefUrl for URL construction
                    href_ck.attr('href', hrefUrl);
                }

                var href_ck2 = $('#idsanpham_href2_' + value.ID);
                if (href_ck2.length > 0) {
                    var href2 = value.UID + '?buy=1';
                    href_ck2.attr('href', href2);
                }


                });
        });
    });

</script>
}

<main class="main-content">
    <div class="Content-mobile gap-20px">
        <section class="feature-section">
            <img src="~/Content/inoi/img/mobile/Flip-feature-banner-img.png?@DateTime.Now" alt="">
            <img src="~/Content/inoi/img/mobile/Flip-feature-two-banner.png?@DateTime.Now" alt="">
        </section>
        <section class="feature-section one gap-20px section-item-title w-100-32px">
            <div class="w-100 flex-column-center gap-20px">
                <h1 class="one-item-title">Về INOI</h1>
                <hr>
                <div class="d-flex-center gap-20px ">
                    <img class="w-25" src="~/Content/inoi/img/mobile/Frame 263.png?@DateTime.Now" alt="">
                    <p class="w-75 text-frame">INOI là một thương hiệu điện tử trẻ,
                        có tầm nhìn xa với khát vọng rõ ràng là mở rộng hoạt động kinh doanh trên toàn cầu.
                        Chúng tôi đã có hàng triệu khách hàng trên toàn thế giới.​ </p>
                </div>
                <hr>
                <div class="d-flex-center gap-20px ">
                    <img class="w-25" src="~/Content/inoi/img/mobile/Frame 264.png?@DateTime.Now" alt="">
                    <p class="w-75 text-frame">Chúng tôi mang đến thị trường những thiết bị chất lượng cao, giá cả phải
                        chăng.
                        Danh mục sản phẩm của chúng tôi trải rộng từ điện thoại thông minh, máy tính bảng đến điện thoại
                        phổ thông – để giúp mọi người có thể kết nối một cách dễ dàng.
                        Bên cạnh đó, chúng tôi cũng đang phát triển công nghệ tài chính (fintech) để đáp ứng được nhu
                        cầu của người dùng hiện đại.</p>
                </div>
                <hr>
                <div class="d-flex-center gap-20px ">
                    <img class="w-25" src="~/Content/inoi/img/mobile/Frame 265.png?@DateTime.Now" alt="">
                    <p class="w-75 text-frame">Ban lãnh đạo bao gồm các cựu giám đốc điều hành của Samsung và Nokia, tái
                        khẳng định cam
                        kết về chất lượng sản phẩm cũng như mục tiêu mở rộng thị trường toàn cầu.</p>
                </div>
            </div>

